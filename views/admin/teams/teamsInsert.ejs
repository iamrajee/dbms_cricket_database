<div id = "insertionForm">
        <form action="/", id = "teamsInsertForm">
            Team Name: <br>
            <input type="text" name = "series_id" placeholder="Enter" id = "series_id" required>
            <br>
            Authority: <br>
            <input type="text" name = "stadium_id" placeholder="Enter" id = "stadium_id" required>
            <br>
        </form>
    
        <style>
            #insertionForm{
                display: inline-block;
                padding: 10px;
            }
        
            input[type=text]{
                padding:7px 5px 7px 5px;
                border: 0;
                font-size:medium;
                border: 1px solid gray;
                border-radius: 4px;
            }
        
            input[type=submit]{
                margin-top: 10px;
                margin-bottom: 10px;
                padding: 8px;
                border:0;
                border: 1px solid rgb(13, 13, 201);
                border-radius: 2px;
                color: white;
                background-color: rgb(13, 13, 201);
            }
        </style>
        
        <script>
            $(document).ready(()=> {
                $('#teamsInsertForm').submit(event => {
                    event.preventDefault()
                    const formData = $('#teamsInsertForm').serialize()
        
                    $.ajax({
                        type: 'POST',
                        url: 'admin/insert/Countries',
                        data: formData,
                        contentType: 'application/x-www-form-urlencoded',
                        success: (data)=>{
                            console.log("Data Inserted")
                        },
                        error: (err) => {
                            console.log(err)
                        }
                    })
                })
            })
        </script>
    </div>